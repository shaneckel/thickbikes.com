{
  "name": "thickbikes.com",
  "version": "1.0.0",
  "description": "website for a bike shop in Pittsburgh",
  "author": "shane eckel",
  "scripts": {
    "dev": "nodemon --exec babel-node -w server.js -w nuxt.config.js -w api/ server.js",
    "build": "nuxt build && babel server.js --out-file server.prod.js",
    "start": "cross-env NODE_ENV=production node server.prod.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "axios": "^0.15.3",
    "cross-env": "^3.1.4",
    "express": "^4.14.0",
    "nuxt": "~0.10.5"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "nodemon": "^1.11.0"
  }
}
